{
  "name": "vue-api-queries",
  "version": "0.0.1",
  "description": "Elegant and simple way to build requests for REST API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nuxt-ts demo",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watchAll",
    "test:watch:jsdom": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm link && nodemon",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Chantouch/vue-api-queries.git"
  },
  "keywords": [
    "vue",
    "nuxt",
    "api",
    "rest",
    "query",
    "builder",
    "laravel"
  ],
  "author": {
    "name": "Chantouch Sek",
    "email": "chantouchsek.cs83@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chantouch/vue-api-queries/issues"
  },
  "homepage": "https://github.com/Chantouch/vue-api-queries#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/auth-next": "^5.0.0-1595976864.509d9d6",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/jest": "^26.0.14",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "axios-mock-adapter": "^1.18.2",
    "bootstrap-vue": "^2.17.3",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "nuxt-edge": "latest",
    "prettier": "^2.1.2",
    "semantic-release": "^17.1.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.3",
    "vue-api-queries": "file:."
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "axios": "^0.20.0",
    "camelcase-keys": "^6.2.2",
    "escape-string-regexp": "^4.0.0",
    "lodash.merge": "^4.6.2",
    "snakecase-keys": "^3.2.0"
  }
}
